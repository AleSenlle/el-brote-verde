{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://trefle.io/api/v1/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/src/assets/:path*",
      "destination": "/:path*",
      "permanent": true
    }
  ]
}